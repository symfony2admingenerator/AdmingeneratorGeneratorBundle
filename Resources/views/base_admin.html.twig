<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{% block title %}Admin{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% block scripts_head %}
        {% javascripts filter='?yui_js' output='assetic/js/compiled/admingenerator/jquery.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-1.8.3.min.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
            <script src="{{ asset('bundles/admingeneratorgenerator/js/html5shiv/html5.js') }}"></script>
        <![endif]-->
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets filter='cssrewrite,?yui_css' output='assetic/css/compiled/admingenerator/bootstrap.min.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/bootstrap/bootstrap.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/bootstrap/bootstrap-extended.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/bootstrap/bootstrap-datepicker.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/google/prettify.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
        {% endstylesheets %}

        {% stylesheets filter='cssrewrite,?yui_css' output='assetic/css/compiled/admingenerator/bootstrap-responsive.min.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/bootstrap/bootstrap-responsive.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/bootstrap/bootstrap-responsive-extended.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
        {% endstylesheets %}

        {% stylesheets filter='cssrewrite,?yui_css' output='assetic/css/compiled/admingenerator/admingenerator.min.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/general.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/nested-list.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/collection.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/upload.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/filetypeicons.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/form/datepicker.css'
            '@AdmingeneratorGeneratorBundle/Resources/public/css/admingenerator/form/single-upload.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
        {% endstylesheets %}
    {% endblock %}

    {% block icons %}
        <!-- Favicon and touch icons -->
        <link rel="shortcut icon" href="{{ asset('bundles/admingeneratorgenerator/ico/favicon.ico') }}">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-144-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-114-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-72-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-57-precomposed.png') }}">
    {% endblock %}
  </head>

  <body>

    {% block navbar %}
      {{ include('AdmingeneratorGeneratorBundle::base_admin_navbar.html.twig') }}
    {% endblock navbar %}

    <div class="container-fluid">
      {% block flashes %}
        <div id="flashes" class="row-fluid">
          {% for type, flashMessages in app.session.flashbag.all() %}
            {% for flashMessage in flashMessages %}
              <div class="alert alert-{{ type }}">
                <button type="button" class="close" data-dismiss="alert">Ã—</button>
                {{ flashMessage|raw }}
              </div>
            {% endfor %}
          {% endfor %}
        </div><!--/.row-fluid -->
      {% endblock %}

      {% block body %}
        <div class="row-fluid">
        </div><!--/.row-fluid -->
      {% endblock %}

      {% block footer %}
      <footer>
        <hr>
        <p>{% block footer_text %}&copy; Symfony2Admingenerator {{ ("now"|date('Y') > 2012) ? '2012 - ' ~ "now"|date('Y') : '2012' }}{% endblock %}</p>
      </footer>
      {% endblock %}
    </div> <!--/.container-fluid -->

    {% block javascripts %}
        {% javascripts filter='?yui_js' output='assetic/js/compiled/admingenerator/bootstrap.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-transition.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-alert.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-modal.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-dropdown.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-scrollspy.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-tab.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-tooltip.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-popover.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-button.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-collapse.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-carousel.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-typeahead-extended.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-affix.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/bootstrap-datepicker.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/bootstrap/locales/*'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/google/prettify.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts filter='?yui_js' output='assetic/js/compiled/admingenerator/jquery-ui.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.core.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.widget.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.mouse.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.draggable.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.droppable.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.sortable.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.effect.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/jquery-ui/jquery.ui.effect-slide.js'

        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts filter='?yui_js' output='assetic/js/compiled/admingenerator/blueimp.jquery-file-upload.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/blueimp.javascript-templates.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/blueimp.javascript-load-image.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/blueimp.canvas-to-blob.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/jquery.iframe-transport.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/jquery.fileupload.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/jquery.fileupload-fp.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/blueimp/jquery.fileupload-ui.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts filter='?yui_js' output='assetic/js/compiled/admingenerator/jquery.spin.min.js'
            '@AdmingeneratorGeneratorBundle/Resources/public/js/spin/jquery.spin.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

  </body>
</html>
