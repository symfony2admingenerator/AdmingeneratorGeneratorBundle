{% block column_datetime %}
    {%- if builder.generator.ListParams.use_localized_date == true -%}
        {{- echo_twig(builder.ModelClass ~ '.' ~ column.getter ~ '|localizeddate("medium", "medium")') -}}
    {%- else -%}
        {{- echo_twig(builder.ModelClass ~ '.' ~ column.getter ~ '|date("' ~ builder.generator.ListParams.datetime_format ~ '")') -}}
    {%- endif -%}
{% endblock %}
