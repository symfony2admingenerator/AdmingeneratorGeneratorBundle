{% extends '../CommonAdmin/ShowAction/ShowBuilderAction.php.twig' %}
{% block getObject -%}
    protected function getObject($pk)
    {
        $pk = is_numeric($pk) ? intval($pk) : $pk;

        return $this->getDocumentManager()
                    ->getRepository('{{ model }}')
                    ->find($pk);
    }
{% endblock %}
