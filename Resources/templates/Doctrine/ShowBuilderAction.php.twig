{% extends '../CommonAdmin/ShowAction/ShowBuilderAction.php.twig' %}
{% block getObject -%}
    protected function getObject($pk)
    {
        return $this->getDoctrine()
                    ->getManager({% if (entity_manager is defined) and (entity_manager != null) %}'{{ entity_manager }}'{% endif %})
                    ->getRepository('{{ model }}')
                    ->find($pk);
    }
{% endblock %}
