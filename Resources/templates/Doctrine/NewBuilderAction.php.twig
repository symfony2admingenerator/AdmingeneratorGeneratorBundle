{% extends '../CommonAdmin/NewAction/NewBuilderAction.php.twig' %}

{% block saveObject -%}
    protected function saveObject(\{{ model }} ${{ builder.ModelClass }})
    {
        $em = $this->getDoctrine()->getManager({{ entity_manager|default('')|wrap('"') }});
        $em->persist(${{ builder.ModelClass }});
        $em->flush();
    }
{% endblock %}
